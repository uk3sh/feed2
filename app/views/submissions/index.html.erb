<p id="notice"><%= notice %></p>

<h1>Submissions</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Contact No</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submissions.each do |submission| %>
      <tr>
        <td><%= submission.contact %></td>
        <td><%= link_to 'Show', form_submission_path(@form, submission), remote: true %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<div id="answer-form" class="card card-body bg-light" style="display:none;">
</div>
