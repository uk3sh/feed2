<div class="container">
  <div class="row">
    <div class="col-12 col">
      <%= form_for @account do |f| %>
        
        
        <div class="form-group">
          <%= f.label :sender_id, "Sender ID" %>
          <%= f.text_field :sender_id, class: 'col col-md form-control', placeholder: "Enter a SIX letter word that represents your company, Ex:FEDYFY" %>
        </div>

        <div class="form-group">
          <%= f.label :sender_email %>
          <%= f.text_field :sender_email, class: 'col col-md form-control', placeholder: "Enter from email address" %>
        </div>        

        <div class="form-group">    
          <%= f.submit class: 'btn btn-success'%>
        </div>   
      
      <% end %>
    </div>
  </div>
</div>
